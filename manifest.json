{
    "manifest_version": 2,
    "name": "snakes-on-a-screen",
    "version":"0.1",
    "permissions": ["activeTab","storage"],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["p5.js","sketch.js"]
        }
    ],
    "background": {
        "scripts" :["background.js"]
    },

    "browser_action" : {
        "default_popup":"popup.html"
    },

    "icons": {
        "16": "snake_icon16.png",
        "48": "snake_icon48.png",
        "128": "snake_icon128.png"
    }
}